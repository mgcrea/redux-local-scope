{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [2,  "never"],
    "max-len": [2, 120, 2],
    "no-underscore-dangle": [2, {"allow": ["_id"]}],
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": [2, "nofunc"],
    "object-curly-spacing": [2,  "never"],
    "import/no-extraneous-dependencies": 0
  }
}
